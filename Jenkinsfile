pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo '------------starting--------------------'
            }
        }
        
        stage('Run') {
            steps {
                echo 'Download source from git'
                echo 'Downloaded successfull'
            }
        }
        
        stage('Done') {
            steps {
                echo 'Done'
            }
        }
    }
    
}
